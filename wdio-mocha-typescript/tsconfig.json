{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ],
      "src/*": [
        "./src/*"
      ]
    },
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "types": ["webdriverio","mocha", "node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }

}
